{% extends '::base.html.twig' %}

{% block body -%}
    <h1>MultimediaObject</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Rank</th>
                <td>{{ entity.rank }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ entity.status }}</td>
            </tr>
            <tr>
                <th>Record_date</th>
                <td>{{ entity.recorddate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Public_date</th>
                <td>{{ entity.publicdate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ entity.title }}</td>
            </tr>
            <tr>
                <th>Subtitle</th>
                <td>{{ entity.subtitle }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Line2</th>
                <td>{{ entity.line2 }}</td>
            </tr>
            <tr>
                <th>Copyright</th>
                <td>{{ entity.copyright }}</td>
            </tr>
            <tr>
                <th>Keyword</th>
                <td>{{ entity.keyword }}</td>
            </tr>
            <tr>
                <th>Duration</th>
                <td>{{ entity.duration }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('multimediaobject') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('multimediaobject_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('multimediaobject_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}

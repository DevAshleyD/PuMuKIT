<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Pumukit2!{% endblock %}</title>
        {% block stylesheets %}
          <link href="{{ asset('bundles/pumukitschema/css/bootstrap.css') }}" rel="stylesheet" type="text/css" />
          <link href="{{ asset('bundles/pumukitschema/css/bootstrap-glyphicons.css') }}" rel="stylesheet" type="text/css" />
          <link href="{{ asset('bundles/pumukitschema/css/base.css') }}" rel="stylesheet" type="text/css" />
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <div class="container-fluid">
          {{ include('PumukitSchemaBundle::menu.html.twig') }}
        </div>
           
          <div class="row-fluid" id="body-wrapper">
            {% block body %}{% endblock %}
          </div>
        
        {% block javascripts %}
            <script src="{{ asset('bundles/pumukitschema/js/jquery-2.0.3.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/pumukitschema/js/bootstrap.js') }}" type="text/javascript"></script>
        {% endblock %}
    </body>
</html>

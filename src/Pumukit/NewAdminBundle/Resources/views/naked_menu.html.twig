{% set route_name = app.request.get('_route') %}
{% if route_name == 'pumukitnewadmin_series_index' %}
  <a title="{% trans %}Go to playlists{% endtrans %}" class="btn btn-pumukit btn-raised" href="{{url('pumukitnewadmin_playlist_index')}}">
    {% trans %}Go to playlists{% endtrans %} >
    <div class="ripple-wrapper"></div>
  </a>
{% elseif route_name == 'pumukitnewadmin_playlist_index' %}
  <a title="{% trans %}Go to series{% endtrans %}" class="btn btn-pumukit btn-raised" href="{{ url('pumukitnewadmin_series_index')}}">
    {% trans %}Go to series{% endtrans %} >
    <div class="ripple-wrapper"></div>
  </a>
{% endif %}

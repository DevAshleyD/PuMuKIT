.checkbox {
  margin-top: 0px;
  margin-bottom: 0px;
}

.checkbox label .check {
  width: 15px;
  height: 15px;
  top: 0px;
  left: 0px;
  margin-right: 0px;
}
.checkbox label .check:after {
  left: 0px;
  top: 0px;
  height: 15px;
  width: 15px;
}

.checkbox label .ripple {
  left: 0px;
  top: -9px;
  height: 30px;
  width: 30px;
}

.pagination {
  margin: 0px 0px;
}

.pagination > .active > span,
.pagination > .active > a,
.pagination > .active > span:hover,
.pagination > .active > a:hover {
    background: #009587;
    border-color: #009587;
    color: #fff;
}
.pagination > li > a,
.pagination > li > span, 
.pagination > li > a:hover,
.pagination > li > span:hover {
    color: #009587;
}

<div class="well">
  <div class="row">
    <div class="col-md-6">
      <form class="form-horizontal">
        <fieldset>
          <legend>Info</legend>
          <div class="form-group">
            <label for="inputROId" class="col-lg-2 control-label">Id:</label>
            <div class="col-lg-10 input-group">
              <input type="text" class="form-control" id="inputROId" readonly onclick="select()" value="{{ series.id }}">
            </div>
            <label for="inputROId" class="col-lg-2 control-label">Url:</label>
            <div class="col-lg-10 input-group">
              <input type="text" class="form-control" id="inputROId" readonly onclick="select()" value="{{ url('pumukit_webtv_series_index', {id: series.id}) }}">
              <span class="input-group-addon"><a href="{{ url('pumukit_webtv_series_index', {id: series.id}) }}" target="_blank"><i class="mdi-action-exit-to-app"></i></a></span>
            </div>
            <label for="inputROId" class="col-lg-2 control-label" data-toggle="tooltip" data-placement="bottom" title="{% trans %}This URL will give you access to the series without login.{% endtrans %}">Magic Url:</label>
            <div class="col-lg-10 input-group">
              <input type="text" class="form-control" id="inputROId" readonly onclick="select()" value="{{ url('pumukit_webtv_series_index', {id: series.id}) }}" data-toggle="tooltip" data-placement="bottom" title="{% trans %}This URL will give you access to the series without login.{% endtrans %}"> {#TODO#}
              <span class="input-group-addon"><a href="{{ url('pumukit_webtv_series_index', {id: series.id}) }}" target="_blank"><i class="mdi-action-exit-to-app"></i></a></span>
            </div>
          </div>
        </fieldset>
      </form>
    </div>
  </div>

  <div role="tabpanel">
    <ul id="seriesTab" class="nav nav-tabs" role="tablist">
      <li class="active">
        <a href="#editSeries" aria-controls="editSeries" role="tab" data-toggle="tab">{% trans %}Info{% endtrans %}</a>
      </li>
      <li>
        <a href="#editTemplate" aria-controls="editTemplate" role="tab"data-toggle="tab">{% trans %}Videos Template{% endtrans %}</a>
      </li>
    </ul>
    
    <div class="tab-content">
      <div id="editSeries" role="tabpanel" class="tab-pane active">
        {% include 'PumukitNewAdminBundle:Series:updatemeta.html.twig' %}
      </div>
      <div id="editTemplate" role="tabpanel" class="tab-pane">
        {% include 'PumukitNewAdminBundle:MultimediaObjectTemplate:edit.html.twig' %}
      </div>
    </div>
  </div>

</div>



<div class="well">
  <fieldset>
    <dl>
      <div class="form-row">
        <dt>{% trans %}Images{% endtrans %}:</dt>
        <dd>
          <div id="pic_mms">
	        {% include 'PumukitNewAdminBundle:Pic:list.html.twig' with {'resource': mm, 'resource_name': 'mms'} %}
          </div>
        </dd>
      </div>

      <div class="form-row">
        <dt>{% trans %}Video Files{% endtrans %}:</dt>
        <dd>
          <div id="tracks_mms" style="overflow:visible;">
	        {% include 'PumukitNewAdminBundle:Track:list.html.twig' with {'mmId': mm.id, 'tracks': mm.getTracks(), 'oc': ''} %}
          </div>
        </dd>
      </div>

      <div class="form-row">
        <dt>{% trans %}Materials{% endtrans %}:</dt>
        <dd>
          <div id="materials_mms">
	        {% include 'PumukitNewAdminBundle:Material:list.html.twig' with {'mmId': mm.id, 'materials': mm.getMaterials()} %}
          </div>
        </dd>
      </div>
      
      <div class="form-row">
        <dt>{% trans %}Links{% endtrans %}:</dt>
        <dd>
          <div id="links_mms">
	        {% include 'PumukitNewAdminBundle:Link:list.html.twig' with {'mmId': mm.id, 'links': mm.getLinks()} %}
          </div>
        </dd>
    </div>
    </dl>
  </fieldset>

</div>

<table><tdody>
  {% for link in links %}
    <tr>
      <td><ul><li></li></ul></td>
      <td>
        <a title="{% trans %}Edit file of multimedia object{% endtrans %} {{ link.id }}"
           data-toggle="modal" data-dismiss="modal" data-target="#myModal"
           href="{{ path('pumukitnewadmin_link_update', {'id': link.id, 'mmId': mmId}) }}">
          <i class="mdi-editor-mode-edit"></i>
        </a>
      </td>
      <td>
          <a href="#" onclick="window.submit_delete_link('{{ link.id }}', '{{ mmId }}'); return false;">
             <i class="mdi-content-clear"></i>
          </a>
      </td>
      <td>
        {% if loop.first %}
	  &nbsp;
	{% else %}
	  <a href="#" onclick="$('#links_mms').load('{{ path('pumukitnewadmin_link_up', {'id': link.id, 'mmId': mmId}) }}', {'_method': 'POST'}); return false;">&#8593;</a>
	{% endif %}
      </td>
      <td>
	{% if loop.last %}
          &nbsp;
	{% else %}
	  <a href="#" onclick="$('#links_mms').load('{{ path('pumukitnewadmin_link_down', {'id': link.id, 'mmId': mmId}) }}', {'_method': 'POST'}); return false;">&#8595;</a>
	{% endif %}
      </td>
      <td>&nbsp;{{ link.id }} - {{ link.getUrl() }}</td>
    </tr>
  {% endfor %}
  <tr>
    <td><ul><li></li></ul></td>
    <td colspan="6"><a title="{% trans %}Create Link{% endtrans %}" data-toggle="modal" data-dismiss="modal" data-target="#myModal" href="{{ path('pumukitnewadmin_link_create', {'id': mmId}) }}">{% trans %}create{% endtrans %}...</a></td>
  </tr>
</tbody></table>


<table>
 <tdody>
  {% for person in people %}
    <tr>
      <td><ul><li></li><ul></td>
      <td><a title="{% trans %}Edit Person{% endtrans %} '{{ person.getHName() }}'" onclick="Modalbox.show(this.href, {title:this.title, width:800}); return false;" href="{{ path('pumukitadmin_person_update', {'id': person.id, 'roleId': role.id, 'mmId': mm.id}) }}"><img alt="{% trans %}edit{% endtrans %}" title="{% trans %}edit{% endtrans %}" class="miniTag" src="/legacy/images/admin/mbuttons/edit_inline.gif"></a><td>
      <td>
          <a href="#" onclick="window.submit_delete_person('{{ person.id }}', '{{ role.id }}', '{{ mm.id }}', '{{ person.getHName() }}', '_template'); return false;">
             <img alt="{% trans %}delete{% endtrans %}" title="{% trans %}delete{% endtrans %}" src="/legacy/images/admin/mbuttons/delete_inline.gif">
          </a>
      </td>
      <td>
        {% if loop.first %}
	  &nbsp;
	{% else %}
	  <a href="#" onclick="new Ajax.Updater('{{ role.id }}_person_mms_template', '{{ path('pumukitadmin_person_up', {'id': person.id, 'roleId': role.id, 'mmId': mm.id}) }}', {asynchronous:true, evalScripts:true}); return false;">&#8593;</a>
	{% endif %}
      </td>
      <td>
	{% if loop.last %}
          &nbsp;
	{% else %}
	  <a href="#" onclick="new Ajax.Updater('{{ role.id }}_person_mms_template', '{{ path('pumukitadmin_person_down', {'id': person.id, 'role': role.id, 'mmId': mm.id}) }}', {asynchronous:true, evalScripts:true}); return false;">&#8595;</a>
	{% endif %}
      </td>
      <td>&nbsp;{{ person.id }} - {{ person.getHName() }}</td>
    </tr>
  {% endfor %}
  <tr>
    <td><ul><li></li><ul></td>
    <td colspan="6"><a title="{% trans %}Create{% endtrans %} {{ role.getName() }}" onclick="Modalbox.show(this.href, {title:this.title, width:800}); return false;" href="{{ path('pumukitadmin_person_list_autocomplete', {'roleId': role.id, 'mmId': mm.id}) }}">{% trans %}new{% endtrans %}...</a></td>
  </tr>
 </tbody>
</table>


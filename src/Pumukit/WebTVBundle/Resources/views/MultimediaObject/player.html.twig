<script>
function resizeFrame(elem){
    var width = $(elem).width()
    var height = width/16*9;
    $(elem).height(height);
    console.log('width', width);
    console.log('height', height);
}
$(window).resize(function(){
    resizeFrame($('#paellaiframe'));
});
</script>
<iframe src="{{url('pumukit_videoplayer_index', {'id':multimediaObject.id})}}?id={{multimediaObject.id}}"
        id="paellaiframe"
        frameborder="0"
        border="0"
        width="100%"
        onload="resizeFrame(this)"
        height="500px"
        scrolling="no"
        allowfullscreen>
</iframe>

{% if multimediaObjects|length > 0 %}
    <div class="panel panel-default panel-pmk panel-series">
        <div class="panel-heading">
            {% trans %}Videos from same Series{% endtrans %}
        </div>
        <div class="panel-body">
            {% for multimediaObject in multimediaObjects %}
                {% include 'PumukitWebTVBundle:Resources:template_video_vertical.html.twig' with {'object': multimediaObject} %}
            {% endfor %}
        </div>
    </div>

    <script>
        $( document ).ready(function() {
            var element = document.querySelector('.panel-series .current');
            if (element) { element.scrollIntoView(false); }
        });
    </script>
    
{% endif %}

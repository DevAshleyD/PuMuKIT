<div class="panel panel-default nomargin panel-related">
    <div class="panel-heading">
        {% trans %}Live events{% endtrans %}
    </div>
    <div class="panel-body">
        {% if events|length > 0 %}
            {% for liveevents in events %}
                {% include 'PumukitWebTVBundle:Event:miniMultimediaObjects.html.twig' with {'events' : liveevents, 'livelist': true } %}
            {% endfor %}
        {% endif %}
    </div>
</div>

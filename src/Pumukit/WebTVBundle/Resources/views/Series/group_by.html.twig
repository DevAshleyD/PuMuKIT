{% if template == "date" or template == "date_subserial" %}
    <!--DATE-->
    {% if current_date != last_date %}
        <div class="row">
            <h3> {{ current_date }}</h3>
        </div>
    {% endif %}
{% endif %}

{% if template == "subserial" or template == "date_subserial" %}
    <!-- SUBSERIAL -->
    {% if subserie_title != last_subseriestitle %}
        <div class="row subseries_title_wrapper">
            <h4 class="text-center subseries_title"> {{ subserie_title }} </h4>
        </div>
    {% endif %}
{% elseif template == "multisubserial" %}
    <!-- MULTISUBSERIAL -->
    {% set subseries_now = ( subserie_title | split("\\n") ) %}
    {% set subseries_before = ( last_subseriestitle  | split("\\n") ) %}
    {% for key, subserie_title in subseries_now %}
        {% if subserie_title is not same as ( subseries_before[key] | default('') ) %}
            <div class="row">
                <h4 class="text-center"> {{ subserie_title }} </h4>
            </div>
        {% endif %}
    {% endfor %}
{% elseif template == "place_subserial" %}
    {% if precinct.getTitle() != last_precinct|default('') and series_precinct.getCod() != "PLACE0001PRECINCT001"  and subserie_title != last_subseriestitle|default('') %}

        {% if current_date != last_date %}
            <div class="row subseries_title_wrapper place_subserial">
                <h4 class="text-center subseries_title"> {{ precinct.getTitle() }} </h4>
            </div>
        {% endif %}
        {% if subserie_title is not empty or current_date != last_date %}
            <div class="row subseries_title_wrapper">
                <h4 class="text-center subseries_title"> {{ subserie_title }} </h4>
            </div>
        {% endif %}
        {% set last_precinct = precinct.getTitle() %}
    {% elseif current_date != last_date and subserie_title is empty %}
        <div class="row subseries_title_wrapper place_subserial">
            <h4 class="text-center subseries_title"> {{ precinct.getTitle() }} </h4>
        </div>
    {% endif %}
{% endif %}

{% if subseries %}
    <hr>
{% endif %}

<!DOCTYPE html>
<html class="{% block html_class %}{% endblock %}">
  <head>
    {% block meta %}
      <meta charset="UTF-8" />
      <meta name="Keywords" content="{% block keywords %}WebTv, Pumukit{% endblock %}" />
      <meta content="width=device-width, initial-scale=1.0" name="viewport">
      <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    {% endblock %}
    <title>{% block title %}EASTER!!{% endblock %}</title>
    <!--Block with CSS files-->
    {% block stylesheets %}
      <link href="{{ asset('bundles/pumukitresponsivewebtv/css/bootstrap.min.css') }}" type="text/css" rel="stylesheet" media="screen"/>
    {% endblock %}
    <!--Block with Javascript files-->
    {% block javascripts %}
      <script src="{{ asset('bundles/pumukitresponsivewebtv/js/bootstrap.min.js') }}" type="text/javascript"></script>
    {% endblock %}
    {% block favicon %}
      <link rel="icon" type="image/x-icon" href="{{ asset('bundles/pumukitresponsivewebtv/images/ee/favicon.ico') }}" />
    {% endblock %}
  </head>
  <body style="background-color:duck">
    <div class="container-fluid" style="background-color:duck">
      <div class="jumbotron" style="background-color:duck">
        My NAME IS {{ name }}!
        <br>
        DUCKS ARE FUN! :D

        <a  class="thumbnail" >
          <img src="{{ asset('bundles/pumukitresponsivewebtv/images/ee/ducks.jpg') }}">
        </a>
      </div>
    </div>
  </body>
</html>
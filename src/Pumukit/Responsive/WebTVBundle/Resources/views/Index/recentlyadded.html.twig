<div class="col-no-padding col-lg-6">
  <h1 style="font-size: 32px">{% trans %} Recently added {% endtrans %}</h1>
  <div class="view-content">
    {% for object in last %}
      {% if object.isCollection() %}
        <!-- Series -->
        {% include 'PumukitResponsiveWebTVBundle:Misc:series.html.twig' with {'rows': 1 }%}
      {% else %}
        <!-- Multimedia object -->
        {% include 'PumukitResponsiveWebTVBundle:Misc:multimediaobject.html.twig' with {'rows': 1 }%}
      {% endif %}
    {% endfor %}
  </div>
</div>

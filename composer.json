{
    "name": "pumukit/pumukit2",
    "version": "2.5.x-dev",
    "license": "GPL",
    "type": "project",
    "description": "Media Portal",
    "autoload": {
        "psr-4": {
            "Pumukit\\": "src/Pumukit"
        },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" },
        "files": [ "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php" ]
    },
    "require": {
        "php": ">=7.2.0",
        "symfony/symfony": "3.4.*",
        "doctrine/mongodb-odm": "~1",
        "doctrine/mongodb-odm-bundle": "~3",
        "twig/twig": "~2.0",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "^2.6.4",
        "symfony/monolog-bundle": "^3.1.0",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^5.0.0",
        "incenteev/composer-parameter-handler": "^2.0",
        "friendsofsymfony/user-bundle": "~2.1",
        "jms/serializer-bundle": "2.4.*",
        "white-october/pagerfanta-bundle": "^1.2",
        "knplabs/knp-menu-bundle": "~2.0",
        "gedmo/doctrine-extensions": "@stable",
        "jasig/phpcas": "1.3.*",
        "suncat/mobile-detect-bundle": "1.1.*",
        "vipx/bot-detect-bundle": "^4.0",
        "teltek/pmk2-stats-ui-bundle": "1.0.*",
        "alcaeus/mongo-php-adapter": "@stable"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "phpunit/phpunit": "7.*"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "ext-mongo": "1.6.16"
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml",
            "env-map": {
                "opencast_host":  "PUMUKIT2_OPENCAST_HOST",
                "opencast_username": "PUMUKIT2_OPENCAST_USERNAME",
                "opencast_password": "PUMUKIT2_OPENCAST_PASSWORD",
                "opencast_player": "PUMUKIT2_OPENCAST_PLAYER"
            }
        },
        "branch-alias": null,
        "symfony": {
            "require": "3.4.*"
        }
    }
}
